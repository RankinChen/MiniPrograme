/* pages/photo/photo.wxss */

.imgWrap {
  width: 100%;
  height: 100%;
  position: relative;
}

.imgBg {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.title {
  display: block;
  width: 100%;
  height: 20px;
  padding: 10px;
  font-size: 18px;
}
.skip{
  width: 100%;
  height: 20px;
  padding: 10px;
  text-align: center;
  color: red;
  position: absolute;
  bottom: 0;
}

.shishi0 {
  animation: zoomInUp 5s infinite;
}

.shishi1 {
  animation: jello 5s infinite;
}

.shishi2 {
  animation: swing 5s infinite;
}

.shishi3 {
  animation: zoomInLeft 5s infinite;
}

.shishi4 {
  animation: zoomInRight 5s infinite;
}

.shishi5 {
  animation: fadeOutUp 5s infinite;
}

.shishi6 {
  animation: fadeInLeft 5s infinite;
}

.shishi7 {
  animation: fadeInLeft 5s infinite;
}

.shishi8 {
  animation: fadeInLeft 5s infinite;
}

.shishi9 {
  animation: fadeInLeft 5s infinite;
}
